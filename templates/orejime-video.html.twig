<template class="orejime_embed orejime_template" data-orejime-embeded-id="{{ contentID }}" data-orejime-consent="{{ orejime_consent }}">
  {{ original|raw }}
</template>
{% set classes = [
  'orejime_placeholder',
] %}
<div{{ attributes.addClass(classes) }} data-orejime-embeded-id="{{ contentID }}" data-orejime-consent="{{ orejime_consent }}">
  <div class="orejime_embed">
    <div class="orejime_content">
      <div class="orejime_cookie-icon"></div>
      <p>{{"To watch this video, please"|t}} <a href="#" class="orejime_loadconsent">{{"accept the cookie policy"|t}}</a></p>
    </div>
  </div>
</div>
